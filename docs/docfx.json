{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Igloo",
      "_appFooter": "Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a> - <a href=\"https://game-icons.net/1x1/lorc/cold-heart.html\">Icon</a> by <a href=\"https://lorcblog.blogspot.com\" rel=\"author\">Lorc</a>",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}